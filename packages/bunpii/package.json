        "registry": "https://registry.npmjs.org"
    },
    "main": "main.js",
    "exports": {
        ".": "./main.js"
    },
    "scripts": {
        "r": "bun publish --registry=https://registry.npmjs.org --access=public"
    },
    "keywords": [
        "bun",
        "api",
        "framework",
        "core",
        "javascript",
        "backend",
        "rest",
        "http"
    ],
    "author": "chensuiyi <bimostyle@qq.com>",
    "homepage": "https://chensuiyi.me",
    "license": "Apache-2.0",
    "files": [
        "apis/",
        "checks/",
        "config/",
        "libs/",
        "plugins/",
        "schema/",
        "utils/",
        ".gitignore",
        ".npmignore",
        ".bunignore",
        ".editorconfig",
        ".npmrc",
        ".prettierignore",
        ".prettierrc",
        "LICENSE",
        "main.js",
        "package.json",
        "README.md",
        "vitest.config.js"
    ]
